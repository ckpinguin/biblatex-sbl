\newbibmacro*{setunitrelateddelim}{%
  \ifcsundef{relateddelim\strfield{relatedtype}}
    {\setunit{\relateddelim}}
    {\setunit{\csuse{relateddelim\strfield{relatedtype}}}}}

% create a version of \printunit that expands its arguments
% so we can use \csname inside it
\ExplSyntaxOn
\cs_new_protected:Nn \sbl_printunit:n {
  \printunit{#1}}
\cs_generate_variant:Nn \sbl_printunit:n { x }
\cs_new_eq:NN \eprintunit \sbl_printunit:x
\ExplSyntaxOff

\newbibmacro*{printunitrelatedpunct}{%
  \ifcsundef{relatedpunct\strfield{relatedtype}}
    {\printunit{\relatedpunct}}
    {\eprintunit{\csuse{relatedpunct\strfield{relatedtype}}}}}

\renewbibmacro*{begrelated}{%
  \global\booltrue{bbx@inset}}

\renewbibmacro*{endrelated}{%
  \global\boolfalse{bbx@inset}%
  \usebibmacro*{savehash}}

\renewbibmacro*{related:default}[1]{%
  \entrydata*{#1}{%
    \restorefield{pagination}{\blx@pagination}%
    \usedriver
      {\ifnameundef{savedauthor}
         {\ifnameundef{savededitor}
            {}
            {\ifnamesequal{editor}{savededitor}
               {\clearname{editor}}
               {}}}
         {\ifnamesequal{author}{savedauthor}
            {\clearname{author}%
             % make sure editor isn't printed before title
             \renewbibmacro*{editor+others}{}}
            {}}%
       \renewbibmacro*{related:init}{%
         \ifnameundef{savedauthor}
           {}
           {\restorename{author}{\abx@name@savedauthor}}%
         \ifnameundef{savededitor}
           {}
           {\restorename{editor}{\abx@namesavededitor}}}%
       \DeclareNameAlias{sortname}{default}}%
      {\ifbibliography
         {\thefield{entrytype}}
         {cite:\thefield{entrytype}}}%
    \usebibmacro{setunitrelateddelim}%
    \usebibmacro{related}}}

\newbibmacro*{related:reprint}[1]{%
  \entrydata*{#1}{%
    \restorefield{pagination}{\blx@pagination}%
    \usedriver
      {\renewbibmacro*{related:init}{}%
       \ifbibliography
         {\renewbibmacro*{parens+publisher+location+date}{%
            \newunit
            \usebibmacro{publisher+location+date}}}
         {}}
      {\ifbibliography
         {\thefield{entrytype}}
         {cite:\thefield{entrytype}}}}}

\newbibmacro*{related:ancienttext}[1]{%
  \global\togglefalse{cbx:relatedshorthand}%
  \global\togglefalse{cbx:shorthandprinted}%
  \togglefalse{blx@citeindex}%
  \ifciteseen{\let\ifciteseen\@firstoftwo}{}%
  \entrydata*{#1}{%
    \restorefield{pagination}{\blx@pagination}%
    \usebibmacro{ifuseshorthand}
      {\usebibmacro{shorthand}%
       \iffieldequalstr{entrysubtype}{RIMA}
         {\restorefield{volume}{\abx@field@number}}
         {}%
       \iffieldundef{volume}
         {}
         {\usebibmacro{setvoldelim}%
          \setunit{\printdelim[\volcontext]{postnotedelim}}%
          \usebibmacro{volume+part}%
          \global\toggletrue{cbx:volprinted}}}
      {\usedriver{}{cite:\thefield{entrytype}}%
       \usebibmacro{ifusevolumeandpart}
         {}
         {\global\toggletrue{cbx:volprinted}}%
       \ifboolexpr{
         test {\ifciteseen}
         and
         not togl {blx@useshorttitle}
       }
         {\global\toggletrue{cbx:shorthandprinted}}
         {}}}}

