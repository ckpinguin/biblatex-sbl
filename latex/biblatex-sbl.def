\NumCheckSetup{\def\mkbibparens#1{#1}}

\renewbibmacro*{postnote}{%
  \ifdefined\postnotecontext
    \setunit{\printdelim[\postnotecontext]{postnotedelim}}%
  \else
    \newunit
  \fi
  \iftoggle{cbx:suppresspostnote}{}{%
    \ifboolexpr{%
      not test {\iffieldundef{postnote}}
      or
      not test {\iffieldundef{pages}}
    }
      {\printfield{postnote}}
      {}}%
  \ifboolexpr{
    togl {cbx:relatedseen}
    or
    not togl {bbx:related}
    or
    test {\iffieldundef{related}}
  }
    {}
    {\usebibmacro{setunitrelateddelim}%
     \usebibmacro{related:init}%
     \usebibmacro{related}}}

\newbibmacro*{pages}{}
\providebibmacro*{pages}{}

\DeclareBibliographyOption[string]{citepages}[permit]{%
  \ifcsdef{cbx@opt@citepages@#1}
    {\csuse{cbx@opt@citepages@#1}}
    {\PackageError{biblatex}
       {Invalid option 'citepages=#1'}
       {Valid values are 'sbl', 'permit', 'omit', 'separate'.}}}

\def\cbx@opt@citepages@sbl{%
  \renewbibmacro*{pages}{%
    \iffieldundef{pages}
      {\ifbool{bbx@inset}
         {}
         {\printfield{postnote}%
          \global\toggletrue{cbx:suppresspostnote}}}
      {\printfield[postnote]{pages}%
       \ifboolexpr{
         test {\iffieldundef{postnote}}
         or
         test {\iffieldpages{postnote}}
         or
         bool {bbx@inset}
       }
         {}
         {\setunit{\addspace}%
          \printtext[parens]{\printfield{postnote}}}}}}

\def\cbx@opt@citepages@permit{%
  \renewbibmacro*{pages}{%
    \iffieldundef{pages}
      {\ifbool{bbx@inset}
         {}
         {\printfield{postnote}%
          \global\toggletrue{cbx:suppresspostnote}}}
      {\printfield[postnote]{pages}%
       \ifboolexpr{
         test {\iffieldundef{postnote}}
         or
         bool {bbx@inset}
       }
         {}
         {\setunit{\addspace}%
          \printtext[parens]{\printfield{postnote}}}}}}

\def\cbx@opt@citepages@omit{%
  \renewbibmacro*{pages}{%
    \iffieldundef{pages}
      {\ifbool{bbx@inset}
         {}
         {\printfield{postnote}%
          \global\toggletrue{cbx:suppresspostnote}}}
      {\ifboolexpr{
         test {\iffieldundef{postnote}}
         or
         bool {bbx@inset}
       }
         {\printfield[postnote]{pages}}
         {\iffieldpages{postnote}
            {\printfield{postnote}}
            {\printfield[postnote]{pages}%
             \setunit*{\addspace}%
             \printtext[parens]{\printfield{postnote}}}}}}}

\def\cbx@opt@citepages@separate{%
  \renewbibmacro*{pages}{%
    \iffieldundef{pages}
      {\ifbool{bbx@inset}
         {}
         {\printfield{postnote}%
          \global\toggletrue{cbx:suppresspostnote}}}
      {\printfield[postnote]{pages}%
       \ifboolexpr{
         test {\iffieldundef{postnote}}
         or
         bool {bbx@inset}
       }
         {}
         {\setunit{\addspace}%
          \printtext[parens]{%
            \iffieldpages{postnote}
              {\bibstring{thiscite}%
               \setunit{\addspace}}
              {}%
            \printfield{postnote}}}}}}

\renewrobustcmd*{\mkvolcitenote}[2]{%
  \ifboolexpr{
    test {\iffieldequalstr{pagination}{none}}
    or
    test {\iffieldundef{pagination}}
  }
    {\printtext{#1}%
     \setunit*{\printdelim[volpostnote]{postnotedelim}}%
     \printtext{#2}}
    {\printtext[volcitevolume]{#1}%
     \setunit*{\printdelim{postnotedelim}}%
     \printtext[volcitepages]{#2}}}

\renewrobustcmd*{\blx@imc@mkpageprefix}[1][pagination]{%
  \begingroup
  \ifstrequal{#1}{pagination}
    {\def\blx@tempa{\blx@mkpageprefix@i}}
    {\def\blx@tempa{\blx@mkpageprefix{page}}}%
  \iffieldundef{#1}
    {}
    {\iffieldequalstr{#1}{none}
       {\def\blx@tempa{\blx@mkpageprefix@i}}
       {\iffieldbibstring{#1}
          {\edef\blx@tempa{\blx@mkpageprefix{\thefield{#1}}}}
          {\blx@warning@entry{%
             Unknown pagination type '\strfield{#1}'}}}}%
  \@ifnextchar[%]
    {\blx@tempa}
    {\blx@tempa[\@firstofone]}}

\newbibmacro*{ifpaginationnone}[2]{%
  \ifboolexpr{
    test {\iffieldequalstr{pagination}{none}}
    or
    test {\iffieldundef{pagination}}
  }
    {#1}
    {#2}}

