       {\ifbool{bbx@inset}
          {\printtext{\bibhyperlink{\strfield{setkey}}
             {\printtext[title]{\printfield[titlecase]{shorttitle}}}}}
          {\printtext[bibhyperlink]{%
             \printtext[title]{\printfield[titlecase]{shorttitle}}}}}%
     \newunit}
    {}}

