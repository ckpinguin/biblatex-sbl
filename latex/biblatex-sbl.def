\newbibmacro*{volume+part+postnote}{%
  \iffieldundef{volume}
    {\usebibmacro{setpostnotedelim}}
    {\usebibmacro{setvoldelim}%
     \setunit{\printdelim[\volcontext]{postnotedelim}}%
     \usebibmacro{volume+part}%
     \usebibmacro{setvolpostnotedelim}}%
  \setunit{\printdelim[\postnotecontext]{postnotedelim}}%
  \ifboolexpr{
    test {\ifciteseen}
    or
    test {\iffieldundef{pages}}
  }
    {\printfield{postnote}}
    {\usebibmacro{pages}}%
  \global\toggletrue{cbx:suppresspostnote}}

\newbibmacro*{related+volume+part+postnote}{%
  \usebibmacro{related:init}%
  \usebibmacro{related}%
  \iffieldundef{volume}
    {\iftoggle{cbx:volprinted}
       {\usebibmacro{volume+part}%
        \usebibmacro{setvolpostnotedelim}}
       {\usebibmacro{setpostnotedelim}}}
    {\usebibmacro{setvoldelim}%
     \setunit{\printdelim[\volcontext]{postnotedelim}}%
     \usebibmacro{volume+part}%
     \usebibmacro{setvolpostnotedelim}}%
  \setunit{\printdelim[\postnotecontext]{postnotedelim}}%
  \ifboolexpr{
    test {\ifciteseen}
    or
    test {\iffieldundef{pages}}
  }
    {\printfield{postnote}}
    {\usebibmacro{pages}}%
  \global\toggletrue{cbx:suppresspostnote}%
  \global\toggletrue{cbx:relatedseen}}

\newbibmacro*{classic:translator+series}{%
  \ifnameundef{translator}
    {}
    {\setunit{\addspace}%
     \printtext[parens]{%
       \printnames[labelname]{translator}%
       \iftoggle{blx@useseries}
         {\newunit
          \usebibmacro{shortseries}}
         {}}}}

% ------------------------------------------------------------------
% ALTERNATE POSTNOTE MACROS
% ------------------------------------------------------------------

\ExplSyntaxOn
\DeclareDocumentCommand{\savepostnotes}{d() u{++}}{
  \IfValueT {#1} { \gdef\postnotefirst{#1} }
  \tl_if_empty:nF {#2} { \gdef\postnotelast{#2} }
}
\cs_new:Npn \_biblatexsbl_splitpostnote:n #1 { \savepostnotes #1++ }
\cs_generate_variant:Nn \_biblatexsbl_splitpostnote:n { o }
\cs_new_eq:NN \splitpostnote  \_biblatexsbl_splitpostnote:o
\cs_new:Npn \_biblatexsbl_volsavepostnotelast:n #1
  { \gdef\volpostnote{{\volvol}{#1}} }
\cs_generate_variant:Nn \_biblatexsbl_volsavepostnotelast:n { o }
\cs_new_eq:NN \volsavepostnotelast \_biblatexsbl_volsavepostnotelast:o
\ExplSyntaxOff

\def\volsplitpostnote#1#2{%
  \gdef\volvol{#1}%
  \gdef\abx@field@volpostnote{#2}%
  \iffieldundef{volpostnote}
    {}
    {\splitpostnote{\abx@field@volpostnote}}}

\def\setuppostnotes{%
  \global\undef\postnotefirst
  \global\undef\postnotelast
  \iftoggle{cbx:involcite}
    {\expandafter\volsplitpostnote\abx@field@postnote}%
    {\splitpostnote{\abx@field@postnote}}}

\renewbibmacro*{prenote}{%
  \iffieldundef{postnote}
    {}
    {\setuppostnotes
     \iftoggle{cbx:involcite}
       {\ifdefined\postnotelast
          \volsavepostnotelast{\postnotelast}%
          \restorefield{postnote}{\volpostnote}%
        \else
          \def\volpostnote{{\volvol}{}}%
          \restorefield{postnote}{\volpostnote}%
        \fi}
       {\restorefield{postnote}{\postnotelast}}%
     \restorefield{altpostnote}{\postnotefirst}}%
  \iffieldundef{prenote}
    {}
    {\printfield{prenote}%
     \setunit{\prenotedelim}}}

\DeclareFieldFormat{altpostnote}{\mkcomprange{#1}}

