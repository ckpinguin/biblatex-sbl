\newbibmacro*{ifneedsetpostnotedelim}[2]{%
  \ifboolexpr{
    test {\ifentrytype{ancienttext}}
    or
    test {\ifentrytype{classictext}}
  }
    {\def\blx@shorthand{true}}
    {}%
  \usebibmacro{ifuseshorthand} 
    {#1}
    {#2}}

\ExplSyntaxOn
\cs_new_protected:Nn \sbl_ifinstringtf:nnnn {
  \tl_if_in:nnTF {#2}{#1}{#3}{#4}}
\cs_generate_variant:Nn \sbl_ifinstringtf:nnnn { nonn }
\cs_new_eq:NN \IfInStringTF \sbl_ifinstringtf:nonn
\ExplSyntaxOff

\newbibmacro*{ifdotcolonsection}[3]{%
  \restorefield{temp}{#1}%
  \ifboolexpr{
    test {\IfInStringTF{.}{#1}}
    or
    test {\IfInStringTF{:}{#1}}
    or
    test {\IfInStringTF{§}{#1}}
    or
    test {\IfInStringTF{\S}{#1}}
    or
    (
      test {\iffieldequalstr{pagination}{section}}
      and
      test {\iffieldpages{temp}}
    )
  }
    {#2}
    {#3}}

\newbibmacro*{setpagesunit:article}{%
  \ifciteseen
    {\setunit{\printdelim{postnotedelim}}}
    {\iffieldundef{pages}
       {\iffieldundef{volume}
          {\setunit{\printdelim{postnotedelim}}}
          {\usebibmacro{ifpaginationnone}
             {\setunit{\printdelim[parenvol]{postnotedelim}}}
             {\setunit{\printdelim{postnotedelim}}}}}
       {\usebibmacro{ifdotcolonsection}{\abx@field@pages}
          {\setunit{\printdelim[shorthandvol]{postnotedelim}}}
          {\iffieldundef{volume}
             {\setunit{\printdelim{postnotedelim}}}
             {\usebibmacro{ifpaginationnone}
                {\setunit{\printdelim[parenvol]{postnotedelim}}}
                {\setunit{\printdelim{postnotedelim}}}}}}}}

\newbibmacro*{setpagesunit:periodical}{%
  \ifciteseen
    {\setunit{\printdelim{postnotedelim}}}
    {\iffieldundef{volume}
       {\setunit{\printdelim{postnotedelim}}}
       {\usebibmacro{ifpaginationnone}
          {\setunit{\printdelim[parenvol]{postnotedelim}}}
          {\setunit{\printdelim{postnotedelim}}}}}}

\newbibmacro*{setpagesunit}{%
  \iffieldundef{pages}
    {\iffieldundef{postnote}
       {\newunit}
       {\iftoggle{cbx:involcite}
          {\usebibmacro{setvolpagesunit}}
          {\usebibmacro{ifdotcolonsection}{\abx@field@postnote}
             {\setunit{\printdelim[shorthandvol]{postnotedelim}}}
             {\newunit}}}}
    {\usebibmacro{ifdotcolonsection}{\abx@field@pages}
       {\setunit{\printdelim[shorthandvol]{postnotedelim}}}
       {\newunit}}}

\newbibmacro*{setvolpagesunit}{%
  \usebibmacro{ifpaginationnone}
    {\setunit{\printdelim[shorthandvol]{postnotedelim}}%
     \gdef\volcitevolformat{volume}}
    {\newunit}}

\newbibmacro*{setpostnotedelim}{%
  \ifboolexpr{
    test {\iffieldundef{postnote}}
    or
    not togl {cbx:shorthandprinted}
  }
    {}
    {\iftoggle{cbx:involcite}
       {\usebibmacro{ifpaginationnone}
          {\gdef\postnotecontext{shorthandvol}}
          {}}
       {\usebibmacro{ifdotcolonsection}{\abx@field@postnote}
          {\gdef\postnotecontext{shorthandvol}}
          {}}}}

\newbibmacro*{setvoldelim}{%
  \iftoggle{cbx:shorthandprinted}
    {\usebibmacro{ifpaginationnone}
       {\gdef\volcontext{shorthandvol}}
       {}}
    {}}

\newbibmacro*{setvolpostnotedelim}{%
  \usebibmacro{ifpaginationnone}
    {\gdef\postnotecontext{volpostnote}}
    {}}

\newbibmacro*{setvolpagespostnotedelim}{%
